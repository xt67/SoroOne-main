    "http-proxy-middleware": "^3.0.5",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "node-fetch": "^3.3.2",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.8.3"
  },
