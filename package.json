{
  "name": "soroone-main",
  "version": "1.2.0",
  "private": true,
  "description": "AI-powered data analytics and visualization mobile app with local Ollama Mistral integration",
  "keywords": [
    "react-native",
    "expo",
    "data-analytics",
    "ai",
    "ollama",
    "mistral",
    "visualization",
    "dashboard",
    "sql-editor"
  ],
  "main": "./index.js",
  "scripts": {
    "start": "expo start",
    "dev": "powershell -ExecutionPolicy Bypass -File ./start-soroone.ps1",
    "start:tunnel": "expo start --tunnel",
    "proxy": "node proxy-server.js",
    "start:with-ollama": "concurrently \"npm run ollama:start\" \"npm run start\"",
    "start:dev": "node ./scripts/start-dev.js",
    "start:auto": "node ./scripts/start-dev.js",
    "daily": "node scripts/daily-maintenance.js",
    "daily:windows": "powershell -ExecutionPolicy Bypass -File ./daily-maintenance.ps1",
    "contribute": "npm run daily",
    "ollama:start": "ollama serve",
    "ollama:pull": "ollama pull mistral",
    "ollama:setup": "npm run ollama:pull && echo Ollama setup complete!",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "npx eslint . --ext .js,.jsx",
    "type-check": "npx tsc --noEmit --skipLibCheck",
    "reset-project": "node ./scripts/reset-project.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "~2.1.0",
